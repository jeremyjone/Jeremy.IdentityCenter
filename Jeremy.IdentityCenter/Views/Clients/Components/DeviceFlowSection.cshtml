@model Jeremy.IdentityCenter.Business.Models.Clients.ClientViewModel

<div class="card mt-3">
    <h5 class="card-header">客户端</h5>
    <div class="card-body">
        <!--Input - text -->
        <div class="form-group row">
            <label-name asp-for="UserCodeType" class="col-sm-3 col-form-label"></label-name>
            <div class="col-sm-9">
                <input type="text" class="form-control" asp-for="UserCodeType">
            </div>
        </div>

        <!--Input - number -->
        <div class="form-group row">
            <label-name asp-for="DeviceCodeLifetime" class="col-sm-3 col-form-label"></label-name>
            <div class="col-sm-9">
                <input type="number" class="form-control" asp-for="DeviceCodeLifetime">
            </div>
        </div>
    </div>
</div>